{
  "manifest_version": 3,
  "name": "Iot Integration Extension for lodgegeek",
  "version": "1.0",
  "description": "Adds Iot integration functionality to calendar page",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://hostex.io/app/calendar"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://67e8a51e20e3af747c41ac3d.mockapi.io/stays/*"
  ]
} 