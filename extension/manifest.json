{
  "manifest_version": 3,
  "name": "SwitchBot Integration Extension",
  "version": "1.0",
  "description": "Adds SwitchBot integration functionality to calendar page",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://hostex.io/app/calendar"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://staycode.free.beeceptor.com/*"
  ]
} 